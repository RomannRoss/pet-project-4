Сложность: легко

#74
SELECT id,
CASE
WHEN has_internet=1 THEN 'YES'
WHEN has_internet=0 THEN 'NO'
END AS has_internet
FROM rooms;

Сложность: средняя

#59
SELECT * 
FROM users
WHERE phone_number LIKE '+375%';

#72
SELECT room_id, CEIL(AVG(price)) AS avg_price
FROM reservations
GROUP BY room_id;

#73
SELECT room_id, COUNT (room_id) AS count
FROM reservations
GROUP BY room_id
HAVING (count % 2) = 1;

#78
SELECT * 
FROM users
WHERE email LIKE '%hotmail.com';
